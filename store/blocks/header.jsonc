{
  "header": {
    "blocks": [
      "header-layout.desktop", 
      "header-layout.mobile"
    ]
  },
  "header.full": {
    "blocks": [
      "header-layout.desktop", 
      "header-layout.mobile"
    ]
  },
  "header-layout.desktop": {
    "children": [
      "header-row#1-desktop",
      "header-row#desktop-bar",
      "sticky-layout#desktop"//,
      // "header-row#menu-desktop"
    ]
  },
  //header bar
  "header-row#1-desktop": {
    "children": [
      "telemarketing"
    ],
    "props": {
      "fullWidth": true
    }
  },
  "header-row#desktop-bar": {
    "children": [
      "header-border",
      "notification.bar#home"
    ],
    "props": {
      "fullWidth": "true"
    }
  },
  "notification.bar#home": {
    "props": {
      "content": "Env√≠os GRATIS a todo Colombia por compras superiores a $100.000, Disponible el servicio de pago contra entrega.",
      "blockClass": "envioGratis"
    }
  },
  //venia por defecto
  "sticky-layout#desktop": {
    "props": {
      "blockClass": "sticky-header"
    },
    "children": ["flex-layout.row#desktop"]
  },
  "flex-layout.row#desktop": {
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true,
      "blockClass": "menu-link",
      "inverted": "true"
    },
    "children": [
      "flex-layout.col#logo-desktop",
      "vtex.menu@2.x:menu#category-menu",
      "flex-layout.col#spacer",
      "search-bar",
      // "locale-switcher",
      "login#prueba",
      "minicart.v2"
    ]
  },
  "login#prueba": {
    "props": {
      "emailAndPasswordTitle": "LOG-IN",
      "accessCodeTitle": "Acess Code LOG-IN",
      "emailPlaceholder": "e-mail",
      "passwordPlaceholder": "password",
      "optionsTitle": "Login",
      "showPasswordVerificationIntoTooltip": true
    }
  },
////////////////menu///////////////////////////
"vtex.menu@2.x:menu#category-menu": {
  "props": {
    "experimentalOptimizeRendering": true
  },
  "children": [
    "menu-item#category-new",
    "menu-item#category-men",
    "menu-item#category-women"
    // "menu-item#about-us"
  ]
},
"menu-item#category-new":{
  "props":{
    "id": "menu-item-category-new",
    "type": "custom",
    "iconId": null,
    "highlight": false,
    "itemProps": {
      "type": "internal",
      "href": "/new/",
      "noFollow": false,
      "tagTitle": "New",
      "text": "NEW"
    },
    "blockClass": "menuTitleNew"
  }
},
