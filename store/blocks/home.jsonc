   //vitrina de productos
  "shelf#home": {
    "blocks":["product-summary.shelf"],
    "props":{
      "productList": {
        "titleText": "NUEVOS PRODUCTOS",
        "itemsPerPage": 4,
        "maxItems": 10,
        "minItemsPerPage": 2,
        "scroll": "BY_PAGE",
        "arrows": "true",
        "blockClass": "vitrina1",
        "showPaginationDots": "never"
    },
