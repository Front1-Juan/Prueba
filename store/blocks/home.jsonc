{
  "store.home": { 
    "blocks": [
      "slider-layout#principal",
      "slider-layout#bannercollection",
      "shelf#home",
      "rich-text#combosTitle",
      "slider-layout#combos"
      // "slider-layout#text-test#dos"
    ]
  },

  "slider-layout#principal": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "showNavigationArrows": "never",
      "showPaginationDots": "never",
      "usePagination": true,
      "slideTransition": {
        "speed": 400,
        "delay": 500,
        "timing": "ease"
      },
      "autoplay": {
        "timeout": 3000,
        "stopOnHover": false
      },
      "blockClass": "carousel"
    },
    "children": [
      "image#uno",
      "image#dos", 
      "image#tres",
      "image#cuatro"
    ]
  },

  "image#uno": {
    "props": {
      "src": "https://cdn.shopify.com/s/files/1/0275/2679/1277/files/BANNER-FIST2.jpg?v=1624556131"
      // "maxHeight": "100%"
    }
  },
  "image#dos": {
    "props": {
      "src": "https://cdn.shopify.com/s/files/1/0275/2679/1277/files/BANNER-FIST9.jpg?v=1624556380"
      // "maxHeight": "100%"
    }
  },
  "image#tres": {
    "props": {
      "src": "https://cdn.shopify.com/s/files/1/0275/2679/1277/files/BANNER-FIST10.jpg?v=1624564987"
      // "maxHeight": "100%"
    }
  },
  "image#cuatro": {
    "props": {
      "src": "https://cdn.shopify.com/s/files/1/0275/2679/1277/files/BANNER-FIST6.jpg?v=1624556441"
      // "maxHeight": "100%"
    }
  },
  
  /////////////////info cards/////////////////////////
  ///////////////////banner collections////////////////////
  "slider-layout#bannercollection": {
    "props": {
      "itemsPerPage": {
        "desktop": 3,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "showNavigationArrows": "never",
      "showPaginationDots": "never",
      "blockClass": "collectionAll"
    },
    
    "children": [
      "info-card#collection-one-desk", 
      "info-card#collection-two", 
      "info-card#collection-three"]
  },

